<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xsi:schemaLocation="http://www.springframework.org/schema/beans

   http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">


<bean id="line" class="com.manipal.assign01.assignment01.Line">

<property name="point1" ref="point1"></property>

<property name="point2" ref="point2"></property>

</bean>


<bean id="triangle" class=" com.manipal.assign01.assignment01.Triangle">

<property name="point1" ref="point1"></property>

<property name="point2" ref="point2"></property>

<property name="point3" ref="point3"></property>

</bean>



<bean id="point1" class=" com.manipal.assign01.assignment01.Point">

<property name="x" value="0"></property>

<property name="y" value="0"></property>

</bean>



<bean id="point2" class=" com.manipal.assign01.assignment01.Point">

<property name="x" value="3"></property>

<property name="y" value="3"></property>

</bean>



<bean id="point3" class=" com.manipal.assign01.assignment01.Point">

<property name="x" value="6"></property>

<property name="y" value="0"></property>

</bean>

</beans>